#: akrypt_icode.c:518
#, c-format
msgid ""
"akrypt icode [options] [files or directories]  - calculation and/or checking "
"integrity codes for given files\n"
"\n"
msgstr ""
"akrypt icode [опции] [файлы или каталоги]  - вычисление и проверка "
"контрольных сумм или имитовставок для заданных файлов\n"
"\n"

#: akrypt_icode.c:519 akrypt_show.c:161
#, c-format
msgid "available options:\n"
msgstr "доступные опции:\n"

#: akrypt_icode.c:520
#, c-format
msgid ""
" -a, --algorithm <ni>    set the algorithm, where \"ni\" is name or "
"identifier of mac or hash function\n"
msgstr ""
" -a, --algorithm <ni>    указать алгоритм вычисления контрольной суммы или "
"имитовставки\n"

#: akrypt_icode.c:521
#, c-format
msgid ""
"                         default algorithm is \"streebog256\" defined by "
"GOST R 34.10-2012\n"
msgstr ""
"                         алгоритмом по-умолчанию является бесключевая "
"функция Стрибог256, регламентируемая ГОСТ Р 34.10-2012\n"

#: akrypt_icode.c:522
#, c-format
msgid ""
" -c, --check <file>      check previously generated macs or integrity codes\n"
msgstr ""
" -c, --check <file>      проверить контрольные суммы или имитовставки, "
"сохраненные в указанном файле\n"

#: akrypt_icode.c:523
#, c-format
msgid ""
"     --dont-show-stat    don't show a statistical results after checking\n"
msgstr ""
"     --dont-show-stat    не показывать статистическую информацию после "
"завершения проверки\n"

#: akrypt_icode.c:524
#, c-format
msgid ""
"     --ignore-errors     don't breake a check when file is missing or "
"corrupted\n"
msgstr ""
"     --ignore-errors     не прерывать выполнение программы при возникновении "
"какой-либо ошибки\n"

#: akrypt_icode.c:525
#, c-format
msgid ""
" -o, --output <file>     set the output file for generated integrity codes\n"
msgstr ""
" -o, --output <file>     указать имя файла для вывода контрольных сумм или "
"имитовставок\n"

#: akrypt_icode.c:526
#, c-format
msgid ""
" -p                      load the password from console to generate a secret "
"key\n"
msgstr ""
" -p                      использовать пароль для генерации ключа "
"имитозащиты\n"

#: akrypt_icode.c:527
#, c-format
msgid "     --password <pass>   set the password directly in command line\n"
msgstr ""
"     --password <pass>   указать пароль для генерации ключа имитозащиты "
"прямо в командной строке\n"

#: akrypt_icode.c:528
#, c-format
msgid ""
"     --quiet             don't print OK for each successfully verified file\n"
msgstr ""
"     --quiet             не печатать Ok после каждого удачно проверенного "
"файла\n"

#: akrypt_icode.c:529
#, c-format
msgid " -r, --recursive         recursive search of files\n"
msgstr ""
" -r, --recursive         использовать рекурсивный обход каталогов для поиска "
"файлов\n"

#: akrypt_icode.c:530
#, c-format
msgid ""
"     --reverse-order     output of integrity code in reverse byte order\n"
msgstr ""
"     --reverse-order     выводить или считывать шестнадцатеричные "
"последовательности в обратном порядке байт\n"

#: akrypt_icode.c:531
#, c-format
msgid ""
"     --status            don't output anything, status code shows success\n"
msgstr ""
"     --status            ничего не выводить, результат определяется кодом "
"возврата\n"

#: akrypt_icode.c:532
#, c-format
msgid "     --tag               create a BSD-style checksum\n"
msgstr "     --tag               выводить результаты вычислений в стиле BSD\n"

#: akrypt_icode.c:533
#, c-format
msgid " -t, --template <str>    set the pattern which is used to find files\n"
msgstr " -t, --template <str>    установить шаблон для поиска файлов\n"

#: akrypt_icode.c:535 akrypt_show.c:169
#, c-format
msgid ""
"\n"
"common akrypt options:\n"
msgstr ""
"\n"
"опции, доступные для всех команд программы:\n"

#: akrypt_icode.c:536 akrypt_show.c:170
#, c-format
msgid ""
"     --audit <file>      set the output file for errors and libakrypt audit "
"system messages\n"
msgstr ""
"     --audit <file>      установить имя файла для вывода ошибок и сообщений "
"системы аудита библиотеки\n"

#: akrypt_icode.c:537 akrypt_show.c:171
#, c-format
msgid ""
"     --help              show this information\n"
"\n"
msgstr ""
"     --help              показать справочную информацию\n"
"\n"

