#: akrypt_show.c:160
#, c-format
msgid ""
"akrypt show [options]  - show useful information about user and libakrypt "
"parameters\n"
"\n"
msgstr ""
"akrypt show [опции]  - показать информацию о параметрах пользователя и "
"библиотеки libakrypt\n"
"\n"

#: akrypt_show.c:162
#, c-format
msgid "     --engines           show all types of available crypto engines\n"
msgstr ""
"     --engines           показать все доступные типы криптографических "
"преобразований\n"

#: akrypt_show.c:163
#, c-format
msgid ""
"     --oid <eni>         show one or more OID's, where \"eni\" is an engine, "
"name or identifier of OID\n"
msgstr ""
"     --oid <eni>         показать один или несколько типов/имен/"
"идентификаторов, включающих в себя заданную последовательность\n"

#: akrypt_show.c:164
#, c-format
msgid ""
"     --oids              show the list of all available libakrypt's OIDs\n"
msgstr ""
"     --oids              показать все доступные идентификаторы библиотеки\n"

#: akrypt_show.c:165
#, c-format
msgid ""
"     --options           show the list of all libakrypt's cryptographic "
"options and their values\n"
msgstr ""
"     --options           вывести список всех криптографических параметров "
"библиотеки и их значений\n"

#: akrypt_show.c:166
#, c-format
msgid "     --modes             show all types of cryptographic modes\n"
msgstr ""
"     --modes             показать все доступные режимы криптографических "
"преобразований\n"

#: akrypt_show.c:167
#, c-format
msgid "     --without-caption   don't show a caption for displayed values\n"
msgstr ""
"     --without-caption   не показывать информационный заголовок при выводе "
"запрашиваемых значений\n"
